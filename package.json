{
  "version": "1.0.0-alpha.0",
  "author": "Community for NL Design System",
  "description": "Design system based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "name": "@example/design-system",
  "private": true,
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "workspaces": [
    "./documentation/",
    "./components/",
    "./components/*",
    "./packages/*",
    "./proprietary/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@etchteam/storybook-addon-status": "^4.0.0",
    "@percy/storybook": "^3.3.1",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^6.2.9",
    "@storybook/html": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/theming": "^6.2.9",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "eslint": "7.28.0",
    "eslint-config-prettier": "6.15.0",
    "html-validate": "4.13.1",
    "husky": "6.0.0",
    "lerna": "4.0.0",
    "lint-staged": "10.5.1",
    "markdownlint-cli": "0.27.1",
    "mdx-embed": "^0.0.19",
    "npm-package-json-lint": "5.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^10.2.0",
    "storybook-addon-mdx-embed": "^0.0.19",
    "storybook-design-token": "^1.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "ts-dedent": "^2.1.1",
    "webpack": "^4.46.0"
  },
  "scripts": {
    "build": "build-storybook --output-dir dist/ --static-dir ./documentation/",
    "clean": "rimraf dist/",
    "bootstrap": "lerna bootstrap",
    "lint": "npm-run-all --continue-on-error lint:**",
    "lint:css": "stylelint --allow-empty-input '**/*.css'",
    "lint:js": "eslint --no-error-on-unmatched-pattern '**/*.{js,jsx,ts,tsx}'",
    "lint:html": "find . -type d \\( -name node_modules \\) -prune -false -o -name '*.html' -print0 | xargs -0 npx html-validate",
    "lint:md": "markdownlint '**/*.md'",
    "lint:package-json": "npmPkgJsonLint --allowEmptyTargets '**/package.json'",
    "lint:prettier": "prettier --check .",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:** prettier",
    "lint-fix:css": "stylelint --fix '**/*.css'",
    "lint-fix:js": "eslint --fix --no-error-on-unmatched-pattern '**/*.{js,jsx,ts,tsx}'",
    "lint-fix:md": "markdownlint --fix '**/*.md'",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "start": "node_modules/http-server/bin/http-server dist/",
    "storybook": "start-storybook --port 6006 --static-dir ./documentation/",
    "test": "exit 0"
  },
  "dependencies": {
    "http-server": "^0.12.3"
  }
}
